{
    "compilerOptions": {
      "outDir": "dist",
      "esModuleInterop": true,
      "noImplicitAny": false,
      "strictNullChecks": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "target": "esnext",
      "moduleResolution": "node",
      "strict": false,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
  }
  