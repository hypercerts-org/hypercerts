  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4",
    "wrangler": "3.15.0"
  },
  "private": true,
  "scripts": {
    "dev": "wrangler dev --port 3000",
