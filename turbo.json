{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**",
        ".docusaurus",
        ".cache-loader",
        "out/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
