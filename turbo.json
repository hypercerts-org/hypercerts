{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local", "**.env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["PLASMIC_PROJECT_ID", "PLASMIC_PROJECT_API_TOKEN"],
      "outputs": ["build/**", "dist/**", "out/**", ".docusaurus", ".next/**"]
    },
    "deploy": {
      "dependsOn": ["build", "lint", "test"]
    },
    "dev": {},
    "lint": {},
    "sentry:sourcemaps": {},
    "test": {}
  }
}
