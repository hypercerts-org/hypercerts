{
  "$schema": "https://turbo.build/schema.json",
  // "globalDependencies": ["**/.env.*local", "**.env"],
  "tasks": {
    "build": {
      "outputs": ["build/**", "dist/**", "out/**", ".docusaurus", ".next/**"]
    },
    "deploy": {
      "dependsOn": ["build"],
      "outputs": ["build/**", "dist/**", "out/**", ".docusaurus", ".next/**"]
    },
    "dev": {
      "persistent": true
    },
    "lint": {},
    "sentry:sourcemaps": {},
    "start": {
      "cache": false
    },
    "test": {}
  }
}
