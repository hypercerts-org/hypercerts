{
  "name": "os-observer",
  "version": "0.0.1",
  "description": "Open source impact measurement",
  "repository": "git@github.com:hypercerts-org/hypercerts.git",
  "author": "Hypercerts team",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint --ignore-path ../.gitignore --max-warnings 0 --cache .",
    "lint:prettier": "prettier --ignore-path ../.gitignore --loglevel warn --check **/*.ts",
    "test": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "keywords": [],
  "devDependencies": {
    "jest": "^29.5.0",
    "prisma": "^4.13.0",
    "ts-jest": "^29.1.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "graphql-request": "^6.1.0",
    "prettier": "^2.8.8"
  }
}
