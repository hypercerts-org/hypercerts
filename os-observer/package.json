{
  "name": "@hypercerts-org/os-observer",
  "version": "0.0.1",
  "description": "Open source impact measurement",
  "repository": "git@github.com:hypercerts-org/hypercerts.git",
  "author": "Hypercerts team",
  "license": "Apache-2.0",
  "bin": "./dist/src/cli.js",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "db:migrate": "prisma migrate dev",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "deploy": "yarn db:migrate",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint --ignore-path ../.gitignore --max-warnings 0 --cache .",
    "lint:prettier": "prettier --ignore-path ../.gitignore --loglevel warn --check **/*.ts",
    "start": "ts-node --esm src/cli.ts",
    "test": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/npm-registry-fetch": "^8.0.4",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "supabase": "^1.64.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "chalk": "^5.2.0",
    "dayjs": "^1.11.7",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "npm-registry-fetch": "^14.0.5",
    "ts-adt": "^2.1.2",
    "winston": "^3.9.0",
    "yargs": "^17.7.2"
  }
}
